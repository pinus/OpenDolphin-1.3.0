#else
		<TD NOWRAP COLSPAN="4">${model.getOrderName()}）${stampName}</TD>
#end
	</TR>
#foreach ($item in $model.getClaimItem())
	<TR>
#if ($item.getNumber())
        <TD valign="top">・</TD>
		<TD width="${hints.getWidth()">${item.getName()}</TD>
		<TD NOWRAP> x ${item.getNumber().replaceAll("[^0-9]0$","")}</TD>
		<TD NOWRAP>$!{item.getUnit()}</TD>
#else
        <TD valign="top">・</TD>
		<TD width="${hints.getWidth()">${item.getName()}</TD>
        <TD NOWRAP>　</TD>
        <TD NOWRAP>　</TD>
#end
	</TR>
#end
#if ($model.getMemo())
	<TR>
		<TD COLSPAN="4">$!{model.getMemo()}</TD>
	</TR>
